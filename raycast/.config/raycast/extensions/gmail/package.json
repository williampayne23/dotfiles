{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "gmail",
  "title": "Gmail",
  "description": "A Minimal Gmail Client",
  "icon": "command_icon.png",
  "author": "williampayne23",
  "categories": [
    "Communication",
    "Web"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Show My Emails",
