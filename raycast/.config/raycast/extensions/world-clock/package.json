{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "world-clock",
  "title": "World Clock",
  "description": "Provides a world clock in the menu bar",
  "icon": "command-icon.png",
  "author": "williampayne23",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Menu Bar Clock",
      "description": "Show the menu bar timezone clock",
      "mode": "menu-bar",
      "interval": "1m"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.51.0",
    "@raycast/utils": "^1.4.8",
    "moment-timezone": "^0.5.43"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}