{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "show-text",
  "title": "show_text",
  "description": "Shows provided text (used with deeplinks)",
  "icon": "command-icon.png",
  "author": "williampayne23",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Show Text",
      "description": "Echo the argument text",
      "mode": "view",
      "arguments": [
        {
          "name": "text",
          "title": "Text",
          "description": "Text to show",
          "placeholder": "Text to show",
          "type": "text",
          "required": true
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.47.3"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}